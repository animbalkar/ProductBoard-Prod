    <fields>
        <fullName>LD_Response_Action__c</fullName>
        <description>case(text( Campaign.Type ),
&quot;Content&quot;,&quot;3-Content&quot;,
&quot;Form&quot;,IF(IsPickVal( Status ,&quot;Submitted&quot;),&quot;1-Form&quot;,&quot;3-Newsletter&quot;),
&quot;Webinar&quot;, IF(IsPickVal( Status ,&quot;Attended&quot;),&quot;2-Webinar&quot;,&quot;&quot;),
&quot;Demo Signup / Trial&quot;,&quot;&quot;,
&quot;Advertisement&quot;,&quot;&quot;,
If(Left(text( Status ),1) = &quot;1&quot; || Left(text( Status ),1) = &quot;2&quot;  || Left(text( Status ),1) = &quot;3&quot; ,
Text(Status) &amp; &quot;-&quot; &amp; Text(Campaign.Type ),&quot;&quot;))</description>
        <externalId>false</externalId>
        <formula>IF(IsPickVal(Campaign.Type ,&quot;Demo Signup / Trial&quot;),&quot;&quot;,
IF(Left(text( Status ),1) = &quot;1&quot; || Left(text( Status ),1) = &quot;2&quot;  || Left(text( Status ),1) = &quot;3&quot;,
Text(Status) &amp; &quot;-&quot; &amp; Text(Campaign.Type ),&quot;&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Formula field to determine when to route a record to sales</inlineHelpText>
        <label>LD Response Action-Formula</label>
