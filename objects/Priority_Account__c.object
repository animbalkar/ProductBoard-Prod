    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Object to track Priority + Named Account time frames so that we can report historically on when an account fell into one of these buckets</description>
    <enableActivities>false</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableFeeds>false</enableFeeds>
    <enableHistory>true</enableHistory>
    <enableLicensing>false</enableLicensing>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <label>Priority Account</label>
    <nameField>
        <displayFormat>PA-{00000}</displayFormat>
        <label>Priority Account Name</label>
        <trackHistory>true</trackHistory>
        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Priority Accounts</pluralLabel>
    <searchLayouts>
        <customTabListAdditionalFields>Account__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Priority_Account__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Start_Date__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>End_Date__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Active_Priority_Account__c</customTabListAdditionalFields>
    </searchLayouts>
    <sharingModel>ControlledByParent</sharingModel>
    <visibility>Public</visibility>
    <fields>
        <fullName>Account__c</fullName>
        <description>Field that ties the Priority or Named Account to the Priority Account record. Master-Detail so there can be roll-ups to the account.</description>
        <externalId>false</externalId>
        <label>Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Priority Account</relationshipLabel>
        <relationshipName>Priority_Account</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>End_Date__c</fullName>
        <description>Field that captured the end date of the campaign</description>
        <externalId>false</externalId>
        <label>End Date</label>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Start_Date__c</fullName>
        <description>Field that captured the start date of the campaign</description>
        <externalId>false</externalId>
        <label>Start Date</label>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Priority_Account__c</fullName>
        <description>Field that captures the name of the targeted list, either P1 or P2 with some additional details around the target</description>
        <externalId>false</externalId>
        <label>Priority Account</label>
        <required>true</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>P1 Sales Targets</fullName>
                    <default>false</default>
                    <label>P1 Sales Targets</label>
                </value>
                <value>
                    <fullName>P2 Digital Transformation</fullName>
                    <default>false</default>
                    <label>P2 Digital Transformation</label>
                </value>
                <value>
                    <fullName>P1 Strategic Accounts</fullName>
                    <default>false</default>
                    <label>P1 Strategic Accounts</label>
                </value>
                <value>
                    <fullName>P2 Jira Accounts</fullName>
                    <default>false</default>
                    <label>P2 Jira Accounts</label>
                </value>
                <value>
                    <fullName>P2 Digital-First Healthcare</fullName>
                    <default>false</default>
                    <label>P2 Digital-First Healthcare</label>
                </value>
                <value>
                    <fullName>P2 Traditional Healthcare</fullName>
                    <default>false</default>
                    <label>P2 Traditional Healthcare</label>
                </value>
                <value>
                    <fullName>P2 Finance</fullName>
                    <default>false</default>
                    <label>P2 Finance</label>
                </value>
                <value>
                    <fullName>P3 ICP Experiment - Prospects</fullName>
                    <default>false</default>
                    <label>P3 ICP Experiment - Prospects</label>
                </value>
                <value>
                    <fullName>P3 ICP Experiment - Customers</fullName>
                    <default>false</default>
                    <label>P3 ICP Experiment - Customers</label>
                </value>
                <value>
                    <fullName>P3 ICP Experiment - Dupes?</fullName>
                    <default>false</default>
                    <label>P3 ICP Experiment - Dupes?</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>X6sense_Segment_Name__c</fullName>
        <description>Field to capture the segment for the list in 6sense so that the record and the list can easily be tied back to each other.</description>
        <externalId>false</externalId>
        <label>6sense Segment Name</label>
        <length>255</length>
        <required>false</required>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Active_Priority_Account__c</fullName>
        <description>Field that returns TRUE/FALSE based on the start and end dates of the Priority Account record.</description>
        <externalId>false</externalId>
        <formula>IF(
AND(
NOT(ISBLANK( Start_Date__c )),
OR(ISBLANK(End_Date__c),
End_Date__c &gt;= Today())), TRUE, FALSE)</formula>
        <label>Active Priority Account</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Priority_Account_To_End__c</fullName>
        <description>Field to help us easily target Priority Account records that need to have an End Date Stamped when new Priority Accounts are loaded.</description>
        <externalId>false</externalId>
        <formula>AND( TEXT(Priority_Account__c) &lt;&gt;  TEXT(Account__r.Priority_Account__c) ,
 Active_Priority_Account__c = TRUE)</formula>
        <label>Priority Account To End</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Account_s_Priority_Account_Value__c</fullName>
        <description>Field used to compare the account&apos;s priority value to the priority account record so that we can easily target ending campaigns</description>
        <externalId>false</externalId>
        <formula>TEXT(Account__r.Priority_Account__c)</formula>
        <label>Account&apos;s Priority Account Value</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
