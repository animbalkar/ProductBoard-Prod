<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Stripe Billing flow controls</description>
    <enableFeeds>false</enableFeeds>
    <label>Stripe Billing Screen Flow</label>
    <visibility>Public</visibility>
    <fields>
        <fullName>Disable_Trials__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>If true, users will not be able to set &apos;Trial End Date&apos; in the Stripe BIlling flow</inlineHelpText>
        <label>Disable Trials</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Intro_Screen_Text__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>This setting allows you to override the text shown on the first screen of the Stripe Billing flow.</inlineHelpText>
        <label>Intro Screen Text</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
