    </fields>
    <fields>
        <fullName>Full_Quote_Number__c</fullName>
        <externalId>false</externalId>
        <formula>&apos;Q&apos; + TEXT(YEAR(TODAY()))+ TEXT(MONTH(TODAY())) + TEXT(DAY(TODAY())) +  QuoteNumber</formula>
        <label>Full Quote Number</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Huberts_signature__c</fullName>
        <externalId>false</externalId>
        <formula>BR() + BR() + BR() + BR() + &apos;Hubert Palan&apos;+ BR() + &apos;CEO&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Huberts signature</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Productboard_Name__c</fullName>
        <externalId>false</externalId>
        <formula>&apos;ProductBoard, Inc.&apos; + BR() + &apos;392 Staten Ave&apos; + BR() + &apos;Oakland, CA 94610&apos; + BR() + &apos;USA&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Productboard Name</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Customer_Signature__c</fullName>
        <externalId>false</externalId>
        <formula>BR() + BR() + BR() + BR() + &apos;Name: &apos; + Contact.FirstName + &apos; &apos; + Contact.LastName + BR() +&apos;Date: &apos; + BR()+ &apos;Title: &apos; + Contact.Title</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Customer Signature</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Contract_Start_Date__c</fullName>
