    <fields>
        <fullName>Hubert_Info__c</fullName>
        <externalId>false</externalId>
        <formula>&apos;Hubert Palan&apos; + BR() + &apos;CEO&apos; + BR() + &apos;(415) 578-0808&apos;</formula>
        <label>Hubert Info</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Hubert_Email__c</fullName>
        <defaultValue>&apos;hubert@productboard.com&apos;</defaultValue>
        <externalId>false</externalId>
        <label>Hubert Email</label>
        <required>false</required>
        <type>Email</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Quote_Number__c</fullName>
        <externalId>false</externalId>
        <label>Quote Number</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Contract_Term__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(Contract_Start_Date__c) + &apos; - &apos; +  TEXT(Contract_End_Date__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Contract Term</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Subscriber_Name__c</fullName>
        <externalId>false</externalId>
        <formula>Contact.FirstName + &apos; &apos; + Contact.LastName</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Subscriber Name</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Quote_Salutation__c</fullName>
        <externalId>false</externalId>
        <formula>&apos;Dear &apos; + Contact.FirstName + &apos;,&apos; + BR() + &apos;We are happy to offer you productboard subscription as follows:&apos;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Quote Salutation</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Subscriber_Email__c</fullName>
        <externalId>false</externalId>
