    <decisions>
        <name>Are_there_Products</name>
        <label>Are there Products?</label>
        <locationX>2008</locationX>
        <locationY>1751</locationY>
        <defaultConnector>
            <targetReference>NoOpportunityProducts</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No Products on the Opportunity</defaultConnectorLabel>
        <rules>
            <name>Products_Exist</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Opportunity_Products</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Product_not_Pushed_to_Stripe</leftValueReference>
                <operator>WasVisited</operator>
