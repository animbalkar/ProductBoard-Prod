    <formulas>
        <name>formula_service_start_date_12am</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(TEXT({!Opportunity.Service_Start_Date__c})&amp;&apos; &apos;&amp; LEFT({!constant_12am}, 8))</expression>
    </formulas>
    <formulas>
        <name>formula_year_from_service_start_date_12am</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(TEXT(ADDMONTHS({!Opportunity.Service_Start_Date__c},12))&amp;&apos; &apos;&amp; LEFT({!constant_12am}, 8))</expression>
    </formulas>
    <formulas>
        <name>IntroScreenText</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!Get_Custom_Settings.Intro_Screen_Text__c}), &quot;Hi &quot;+{!$User.FirstName}+&quot;!&quot;+BR()+BR()+&quot; Click &apos;Next&apos; to create a new Stripe Billing Subscription&quot;, {!Get_Custom_Settings.Intro_Screen_Text__c})</expression>
    </formulas>
